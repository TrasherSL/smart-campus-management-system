{
  "name": "smart-campus-managment-system",
  "version": "1.0.0",
  "description": "A comprehensive MERN stack application for managing campus resources, events, scheduling, and communication",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cd frontend && npm run build"
  },
  "keywords": [
    "mern",
    "campus",
    "management",
    "system"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.7",
    "moment": "^2.30.1",
    "mongoose": "^8.12.1",
    "react-bootstrap": "^2.10.9",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.3.0"
  }
} 